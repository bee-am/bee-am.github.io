                "margin-bottom": "48px"
              },
              ".mbr-text": {
                "margin-bottom": "48px"
              },
              "@newButtonAni": "rgba(red(@buttonAni), green(@buttonAni), blue(@buttonAni), @btnOpacity)",
              ".mbr-section-btn": {
                ".btn": {
                  "background-image": "linear-gradient(99deg, rgba(255, 255, 255, 0) 30%, @newButtonAni 100%), radial-gradient(circle at 50% 50%, @newButtonAni 0, rgba(255, 255, 255, 0) 70%)",
                  "color": "@buttonColor !important"
                }
              }
            },
